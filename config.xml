<?xml version='1.0' encoding='utf-8'?>
<widget id="com.terikon.photoLibraryTester" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Photo Library Tester</name>
    <description>
        Tester for cordova-plugin-photo-library
    </description>
    <author email="npm@terikon.com" href="http://il.linkedin.com/in/romanviskin">
        Roman Viskin
    </author>
    <content src="cdvtests/index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="add" spec="http://git-wip-us.apache.org/repos/asf/cordova-plugin-test-framework.git" />
    <engine name="android" spec="~5.2.2" />
    <engine name="browser" spec="~4.1.0" />
    <engine name="ios" spec="~4.2.1" />
    <plugin name="cordova-plugin-photo-library" spec="https://github.com/terikon/cordova-plugin-photo-library.git">
        <variable name="PHOTO_LIBRARY_USAGE_DESCRIPTION" value="Access for automated tests" />
    </plugin>
    <plugin name="cordova-plugin-photo-library-tests" spec="https://github.com/terikon/cordova-plugin-photo-library.git#:/tests" />
</widget>
