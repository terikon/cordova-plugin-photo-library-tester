{
  "name": "cordova-plugin-photo-library-tester",
  "version": "0.0.1",
  "description": "Tester for cordova-plugin-photo-library",
  "main": "index.js",
  "scripts": {
    "prepare": "cordova prepare",
    "android-run-emulator": "cordova run android --emulator",
    "android-run-device": "cordova run android --device",
    "android-refresh-plugins": "cordova platform update android",
    "android-copy-test-images-emulator": "adb -e push test-images /mnt/sdcard/DCIM && adb -e shell am broadcast -a android.intent.action.MEDIA_MOUNTED -d file:///sdcard",
    "android-copy-test-images-device": "adb -d push test-images /mnt/sdcard/DCIM && adb -d shell am broadcast -a android.intent.action.MEDIA_MOUNTED -d file:///sdcard",
    "ios-run-device": "cordova run ios --device",
    "browser-run": "cordova serve",
    "browser-refresh-plugins": "cordova platform update browser",
    "browser-refresh-plugins-and-run": "npm run browser-refresh-plugins && npm run browser-run",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": {
    "name": "Roman Viskin",
    "email": "npm@terikon.com",
    "url": "http://il.linkedin.com/in/romanviskin"
  },
  "repository": {
